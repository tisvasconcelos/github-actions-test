{
  "name": "dependency-map",
  "version": "0.1.0",
  "license": "ISC",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": true,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "findMissingReducers": "ts-node -T --project tsconfig.json src/findMissingReducers.ts",
    "deps": "ts-node -T --project tsconfig.json src/deps.ts",
    "findPages": "ts-node -T --project tsconfig.json src/findPages.ts",
    "lint": "./node_modules/.bin/eslint src --ext .js,.jsx,.ts,.tsx",
    "build": "tsc --project tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "madge": "*",
    "tslib": "~2.3.1"
  },
  "devDependencies": {
    "typescript": "*",
    "@types/node": "14",
    "ts-node": "9.1.1",
    "eslint": "*",
    "@rushstack/eslint-config": "*",
    "@types/madge": "*"
  }
}
